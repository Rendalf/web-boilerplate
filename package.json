{
  "name": "starter",
  "version": "0.0.1",
  "description": "Boilerplate for web applications based on react, redux, typescript and postcss",
  "main": "src/index.tsx",
  "author": "ar.lipatov93@yandex.ru",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --bail --config ./configs/webpack.config.dev.js",
